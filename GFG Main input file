public static void main (String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		
		for(int i=0;i<a;i++)
		{
		    int b=sc.nextInt();
		    int[] arr= new int[b];
		    for(int j=0;j<b;j++)
		    {
		        arr[j]=sc.nextInt();
		        
		    }
		    int n=count(arr);
		    System.out.println(n);
		}
	}
